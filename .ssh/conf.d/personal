Host home-imac.local
    HostName 192.168.0.47
    User nicolas
    IdentityFile /home/nicolas/.ssh/id_rsa
    UserKnownHostsFile=/home/nicolas/.ssh/known_hosts
    IdentitiesOnly=yes
    CheckHostIP=no

Host home-nas.local
    HostName 192.168.0.100
    User nicolas
    IdentityFile /home/nicolas/.ssh/id_rsa
    UserKnownHostsFile=/home/nicolas/.ssh/known_hosts
    IdentitiesOnly=yes
    CheckHostIP=no

Host home-portefaix1.local
    HostName 192.168.0.208
    User portefaix
    IdentityFile /home/nicolas/.ssh/id_rsa
    UserKnownHostsFile=/home/nicolas/.ssh/known_hosts
    IdentitiesOnly=yes
    CheckHostIP=no


