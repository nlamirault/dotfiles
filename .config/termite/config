# [colors]

# # special
# foreground      = #00cc00
# foreground_bold = #00cc00
# cursor          = #00cc00
# background      = #000000

# # black
# color0  = #000000
# color8  = #000000

# # red
# color1  = #55ff55
# color9  = #55ff55

# # green
# color2  = #00cc00
# color10 = #55ff55

